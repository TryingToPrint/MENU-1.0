cls
call :vbanner
set /p delay="Insert the time of waiting >> "
if "%delay%"=="cancel" (goto edit)
cls
call :vbanner
echo 	[94m[*][0m Updating...
(echo wscript.sleep %delay%) >> FILE.vbs
ping -n 2 127.0.0.1 > nul
cls
call :vbanner
echo 	[94m[*][0m Done !
ping -n 2 127.0.0.1 > nul
goto edit

